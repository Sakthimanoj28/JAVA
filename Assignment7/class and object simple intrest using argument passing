class Si
{
  int p;
  int t;
  int r;
  double SI;
  void get(int x,int y,int z)
  {
    p=x;
    t=y;
    r=z;
  }
  void cal()
  {
    SI=(p*t*r)/100;
  }
  void disp()
  {
    System.out.println("output="+SI);
  }
}
class Simpleintrestclassandobject
{
  public static void main(String arg[])
  {
    Si obj=new Si();
    obj.get(10000,2,12);
    obj.cal();
    obj.disp();
  }
}
