class Add
{
  int a;
  int b;
  int c;
  void get(int x,int y)
  {
    a=x;
    b=y;
  }
  void cal()
  {
    c=a+b;
  }
  void disp()
  {
    System.out.println("output="+c);
  }
}
class Testclassandobject3
{
  public static void main(String arg[])
  {
    Add obj=new Add();
    obj.get(10,15);
    obj.cal();
    obj.disp();
  }
}
