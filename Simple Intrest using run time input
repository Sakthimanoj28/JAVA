import java.util.*;
class intrestsc
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
double p,i,t,si;
System.out.println("Enter the principel amount :");
p=sc.nextDouble();
System.out.println("Enter the Intrest Rate :");
i=sc.nextDouble();
System.out.println("Enter the Due :");
t=sc.nextDouble();
si=(p*i*t)/100;
System.out.println("The area of circle is :"+si);
}
}
